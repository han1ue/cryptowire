{
    "rewrites": [
        {
            "source": "/rss.xml",
            "destination": "https://cryptowire-api.vercel.app/rss.xml"
        },
        {
            "source": "/news",
            "destination": "https://cryptowire-api.vercel.app/news"
        },
        {
            "source": "/news/(.*)",
            "destination": "https://cryptowire-api.vercel.app/news/$1"
        },
        {
            "source": "/prices",
            "destination": "https://cryptowire-api.vercel.app/prices"
        },
        {
            "source": "/market",
            "destination": "https://cryptowire-api.vercel.app/market"
        },
        {
            "source": "/health",
            "destination": "https://cryptowire-api.vercel.app/health"
        },
        {
            "source": "/stats",
            "destination": "https://cryptowire-api.vercel.app/stats"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "outputDirectory": "dist"
}
