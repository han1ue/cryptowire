{
    "functions": {
        "api/news/summary/refresh.ts": {
            "maxDuration": 60
        },
        "api/[...path].ts": {
            "maxDuration": 10
        }
    },
    "rewrites": [
        {
            "source": "/",
            "destination": "/api"
        },
        {
            "source": "/(.*)",
            "destination": "/api/$1"
        }
    ]
}
